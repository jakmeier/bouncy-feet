{
  "name": "bouncy-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev-host": "vite dev --host 0.0.0.0",
    "build": "vite build --mode staging",
    "build-prod": "vite build --mode production",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "openapi-generate": "openapi-ts -i peertube-openapi.yaml -o src/lib/peertube-openapi",
    "openapi-download": "wget -O peertube-openapi.yaml https://raw.githubusercontent.com/Chocobozzz/PeerTube/refs/heads/develop/support/doc/api/openapi.yaml"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.83.1",
    "@playwright/test": "^1.51.0",
    "@sveltejs/adapter-node": "^5.2.14",
    "@sveltejs/kit": "^2.29.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.2",
    "bouncy_instructor": "file:./src/lib/instructor",
    "date-fns": "^3.6.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.38.1",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.2",
    "vite": "^6.3.5",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm-esm": "^1.0.3",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.21",
    "@peertube/embed-api": "file:../bouncy_tube/PeerTube/client/src/standalone/embed-player-api",
    "jschannel": "^1.0.2",
    "svelte-gestures": "^5.2.2",
    "svelte-select": "^5.8.3",
    "sveltekit-i18n": "^2.4.2"
  }
}
